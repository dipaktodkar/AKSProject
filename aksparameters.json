{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
    "clusterName": {
      "value": "aks101cluster"
    },
    "location": {
      "value": "[resourceGroup().location]"
    },
    "dnsPrefix": {
      "value": "aks101dns"
    },
    "osDiskSizeGB": {
      "value": 10
    },
    "agentCount": {
      "value": 3
    },
    "agentVMSize": {
      "value": "standard_d2s_v3"
    },
    "linuxAdminUsername": {
      "value": "azureuser"
    },
    "sshRSAPublicKey": {
      "value": "ssh-rsa  generated-by-azure"
    }
}